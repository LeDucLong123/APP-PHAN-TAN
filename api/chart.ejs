<div>
  <canvas id="myChart"></canvas>
</div>

<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

<script>
  const ctx = document.getElementById('myChart');
  const times = <%- JSON.stringify(times) %>;
  const request_count = <%- JSON.stringify(request_count) %>;

  new Chart(ctx, {
    type: 'line',
    data: {
      labels: times,
      datasets: [{
        label: '# of Requests',
        data: request_count,
        borderWidth: 2,
        borderColor: 'rgba(75, 192, 192, 1)',
        fill: false
      }]
    },
    options: {
      scales: {
        y: {
          beginAtZero: true
        }
      }
    }
  });
</script>